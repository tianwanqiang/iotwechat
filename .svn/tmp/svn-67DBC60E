// component/coverView.js
Component({
  /**
   * 组件的属性列表
   */
  properties: {

  },

  /**
   * 组件的初始数据
   */
  data: {
    platNo:'',
    gpsTime:'',
    speed:'',
    location:''
  },

  /**
   * 组件的方法列表
   */
  methods: {
    playCarInfo: function (po, ge, sd, ln){
      this.setData({
        platNo:po,
        gpsTime:ge,
        speed:sd,
        location:ln
      })
    }
  }
})
