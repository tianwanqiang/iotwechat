/*分类*/

.index-nav {
  height: 80px;
  /*background: red;*/
  display: flex;
  flex-wrap: wrap;
  /* flex-direction: row; *//*align-items: center;*//* background-color: #fff; */
  color: #646464;
  margin-top: 15px;
}

.index-nav view {
  /* flex:1; */
  flex-direction: column;
  /* border-right: 1px solid #F2f2f2;  */
}

.l-c-c {
  display: flex;
  justify-content: center;
  align-items: center;
  /* width: 105px;
  height: 89px; */
  width: 33.3%;
  margin-bottom: 10px;
}

.index-nav image {
  width: 60px;
  height: 60px;
}

.index-nav text {
  font-size: 16px;
}

view.text {
  display: flex;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 12px;
  font-size: 15px;
  color: #656565;
}

view.text:before {
  position: absolute;
  display: block;
  content: ' ';
  left: -5px;
  width: 2px;
  height: 100%;
  background-color: #09bb07;
}

.line_y {
  width: 3px;
  height: 100%;
  height: 18px;
  display: inline-block;
  background-color: #09bb07;
}

view.text text {
  margin-left: 10px;
  line-height: 18px;
}

