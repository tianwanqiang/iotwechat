page {
  display: flex;
  flex-direction: column;
  height: 100%;
  background: linear-gradient(to left, #cacaed, #c7d3ff, #ebf8fd);
}

.list01 {
  width: 100%;
  /* padding: 10px 0; */
  border:#eee 1px solid;
  position: relative;
  height: 92%;
}

.list01 scroll-view {
  height: 100%;
  position: absolute;
  left: 0px;
  right: 0px;
  top: 0px;
  bottom: 0px;
}

.radio {
  width: 20%;
  /* height:20%; */
  margin-left: 3%;
  display: inline-block;
  font-weight: bold;
  font-size: 10px;
}

.notShow {
  display: none;
}

radio .wx-radio-input {
  border-radius: 3rpx;
  height: 25rpx;
  width: 25rpx;
  margin-top: -4rpx;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  background-color: #e02e24;
  border: 1rpx solid #e02e24;
  /* 自定义样式.... */
}

.loading {
  padding: 10rpx;
  text-align: center;
}

.loading.complete:before {
  display: none;
}



.zuiwaiceng {
  display: flex;
  width: 85%;
  flex-direction: column;
  box-shadow: 1px 1px 5px #000;
  margin-bottom: 40px;
  border-radius: 10px;
  background: rgba(255, 255, 255, 0.6);
  margin: 10px auto;
}

.castItem {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 10rpx 10rpx 10rpx;
  width: 100%;
}

.castItem-out {
  display: block;
  width: 100%;
  font-size: 12px;
}

.castItem-2 {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0px 60rpx 10rpx 60rpx;
  font-size: 14px;
}

.castDay {
  width: 82%;
  font-size: 15px;
  font-family: Microsoft YaHei;
  color: rgb(92, 165, 165);
}

.buttom_b {
  width: 100%;
  height: 35px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  border-top: 1rpx solid #ccc;
}

.list {
  background: #ccc;
  width: 100%;
  height: 30px;
  padding: 0px 2px;
  font-size: 16px;
}
.mapCls{
height:100%;
width:100%;
position:absolute;
left:0px;
right:0px;
top:0px;
bottom:0px;



}

.containerl {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  font-size: 14px;
  font-family: 'FontAwesome';
  font-weight: normal;
  font-style: normal;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.test {
  height: 100%;
  width: 100%;
  display: inline-block;
}

.index-nav {
  height: 80px;
  display: flex;
  flex-wrap: wrap;
  color: #646464;
  margin-top: 15px;
}

.index-nav view {
  flex-direction: column;
}

.l-c-c {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 33.3%;
  margin-bottom: 10px;
}

.index-nav image {
  width: 60px;
  height: 60px;
}

.index-nav text {
  font-size: 16px;
}

view.text {
  display: flex;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 12px;
  font-size: 15px;
  color: #656565;
}

view.text:before {
  position: absolute;
  display: block;
  content: ' ';
  left: -5px;
  width: 2px;
  height: 100%;
  background-color: #09bb07;
}

.queryInput {
  min-width: 100%;
  white-space: wrap;
  padding: 10px;
  font-size: 14px;
  color: #333;
  flex-direction: row;
  flex-wrap: wrap;
}

.queryView {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.platNolbl {
  font-size: 14px;
  color: #333;
  padding: 10px 0 10px 10px;
}

.platNotxt {
  font-size: 14px;
  color: #333;
  padding: 10px 0 10px 1px;
}

view.text text {
  margin-left: 10px;
  line-height: 18px;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  color: rgb(36, 27, 27);
  font-size: 16px;
}

.swiper-tab-item-left {
  width: 30%;
  border-top: 0.5rpx solid #333;
  border-bottom: 0.5rpx solid #333;
  border-left: 0.5rpx solid #333;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  border-collapse: collapse;
}

.swiper-tab-item-right {
  width: 30%;
  border-top: 0.5rpx solid #333;
  border-bottom: 0.5rpx solid #333;
  border-left: 0.5rpx solid #333;
  border-right: 0.5rpx solid #000;
  border-collapse: collapse;
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
}

.swiper-tab-item {
  width: 30%;
  color: #434343;
}

.swiper-tab-item-soso {
  width: 30%;
  color: #434343;
}

.swiper-tab-item-soso image {
  width: 47rpx;
  height: 47rpx;
  margin-top: 23rpx;
}

.swiper-tab-item-group {
  width: 20%;
  box-shadow: 2rpx 2rpx 3rpx #3b3a3a;
}

.active {
  background-color: #7da4d8;
  font-size: 16px;
  font-weight: bold;
}
.controlsTab{
display:flex;
justify-content:flex-end;
align-items:flex-end;
flex-direction:column;
margin-top:28%;


}

.controlImage{
    box-shadow: 1px 2px 1px rgba(0,0,0,.15);
    cursor: pointer;
    width: 26px;
    height: 26px;
    left: 30px;
    overflow: hidden;
    background-color: #FFF;
    z-index: 10;

}
.controlImage:first-child {
    border-bottom: 1px solid #ccc;
}
.controlImage .out {
    width: 10px;
    height: 10px;
    background-image: url(//webmap0.bdimg.com/wolfman/static/common/images/api/mapZoom.png);
    background-position: -10px 0;
    margin:20rpx auto;

}
.controlImage .in{
    width: 10px;
    height: 10px;
    top: 8px;
    left: 8px;
    background-image: url(//webmap0.bdimg.com/wolfman/static/common/images/api/mapZoom.png);
    background-position: 0 0;
    margin:20rpx auto;

}

.active-group {
  background-color: rgb(140, 178, 236);
  font-size: 16px;
  font-weight: bold;
  color:rgb(192, 93, 53)
}

.show {
  display: block;
}

.hidden {
  display: none;
}
.botomtxt {
  font-size: 31rpx;
}
.btn_track{
  width: 50%;height: 35px;text-align: center;font-size:15px;
  }

.toptxt {
  display: block;
  height: 19rpx;
  margin-bottom: 13rpx;
  margin-top: -15rpx;
  align-content: top;
  text-align: center;
  font-size:33rpx;
}

