/* 底图 */
.bg_img{
  position:absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 100%
}
.bg_img>image{
  width: 100%;
  height: 100%;
}
.index-nav image {
  width: 60px;
  height: 60px;
}

.index-nav text {
  font-size: 36rpx;
}

view.text {
  display: flex;
  align-items: center;
  position: relative;
  padding: 10px 10px 10px 12px;
  font-size: 15px;
  color: #656565;
}

view.text:before {
  position: absolute;
  display: block;
  content: ' ';
  left: -5px;
  width: 2px;
  height: 100%;
  background-color: #09bb07;
}

.line_y {
  width: 3px;
  height: 100%;
  height: 18px;
  display: inline-block;
  background-color: #09bb07;
}
#bg_w{
  width:477rpx;
height:130rpx;
position:absolute;
top:5%;
left:50%;
margin-left:-237rpx;

}
view.text text {
  margin-left: 10px;
  line-height: 18px;
}
page {
  background-color: #fbf9fe;
  /* height: 100%; */
  height:100%;
  overflow-y:hidden;

}

.iconfont {
  font-size: 39rpx;
}


