swiper {
  width: 100%;
  height: 100%;
}

swiper image {
  width: 100%;
  height: 100%;
}

/*分类*/

.index-nav {
  display: flex;
  justify-content: space-between;
  width: 97.5%;
  margin: 0 auto;
  flex-wrap: wrap;
  flex-direction: column;
}

.index-nav view {
  flex-direction: column;
}
.iconfont {
  font-family: "iconfont";
  font-size: 39px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.l-c-c {
  display: flex;
  height: 33.33%;
  justify-content: center;
  align-items: center;
  width: 33.3%;
  background: transparent
}
.l-c-c:nth-child(1){
  width: 33.3%;
  height: 66%;
  align-items: flex-start;
}
.l-c-c:nth-child(2){
  width: 33.3%;
  align-items:flex-start;
}
.l-c-c:nth-child(3){
  width: 66.6%;
  /* height: 66.6%; */
  align-items: flex-end;
}
.l-c-c:nth-child(4){
  width: 66.6%;
  /* height: 66.6%; */
  align-items: flex-end;
}
.l-c-c:nth-child(5){
  width: 66.6%;
  align-items:flex-end; 
}
.l-c-c:nth-child(6){
  align-items: flex-start
}
.l-c-c:nth-child(7){
  width: 66.6%;
  align-items:flex-end; 
}
.l-c-c:nth-child(3),.l-c-c:nth-child(4),.l-c-c:nth-child(5){
}
.l-c-c .index-icon{
   font-size:80rpx;
}
.l-c-c:nth-child(1) .index-icon{
   font-size:70rpx;
}
.l-c-c:nth-child(2) .index-icon{
   font-size:64rpx;
}
.l-c-c-c{
  background: rgba(240, 240, 240, 0.9);
  display:flex;justify-content:center;align-items:center;border-radius:20rpx
}
.l-c-c-c>.index-icon{
  color: #2b73d4;
}
.l-c-c-c>text{
  color: #0d2442;
}
